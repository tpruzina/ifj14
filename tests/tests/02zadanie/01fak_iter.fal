var
	a: integer;
	vysl : integer;
begin
	{write('zadajte cislo pre vypocet faktorialu');
	readln(a);}

	a := 10;

	if a < 0 then
	begin
		write('faktorial nelze spocitat'#10'')
	end
	else
	begin
		vysl := 1;
		while a > 0 do
		begin
			vysl := vysl * a;
			a := a - 1
		end;
		write('Vysledek je ', vysl, ''#10'')
	end
end.
