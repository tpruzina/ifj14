var
	i: integer;
	j: integer;
	x: boolean;
	y: boolean;
	z: boolean;

begin
	for i := 0 to 1 do begin
		if i = 0 then begin
			x := true
		end else begin
			x := false
		end;

		for j := 0 to 1 do begin
			if j = 0 then begin
				y := true
			end else begin
				y := false
			end;

			z := x xor y;
			write(x, ' xor ', y, ' == ',  z, ''#10'')
		end
	end
end.
